script/source = "extends Button
var container
var timer: Timer 
func _ready():
	get_child(0).visible = false
	get_child(0).position.y = -100
	container = $Panel2/FlowContainer
	timer = Timer.new()
	timer.timeout.connect(func(): button_pressed = false)
	add_child(timer)
	mouse_entered.connect(_on_mouse_entered)
	mouse_exited.connect(_on_mouse_exited)
	
	var dir = \"res://addons/m_terrain/brush_masks/\"
	var files = DirAccess.get_files_at(dir)
	for file in files:
		if file.ends_with(\".import\"): continue
		var img = Image.load_from_file(dir + file)
		img.convert(Image.FORMAT_RF)
		var texture_button = TextureButton.new()
		texture_button.ignore_texture_size = true
		texture_button.texture_normal = ImageTexture.create_from_image(img)
		#texture_button.expand_mode = TextureRect.EXPAND_IGNORE_SIZE
		texture_button.stretch_mode= TextureRect.STRETCH_KEEP_ASPECT_CENTERED				
		texture_button.custom_minimum_size = Vector2(100,100)
		container.add_child(texture_button)
		texture_button.mouse_filter = Control.MOUSE_FILTER_PASS
		get_child(0).size.x = files.size() * 100
		
func _toggled(toggled_on):
	if toggled_on:
		get_child(0).visible = true
	else:
		get_child(0).visible = false
		
func update_value(new_value):
	pass	

#func _draw():
#	if button_pressed:
#		draw_circle(Vector2(size.x/2, -get_child(0).size.y), slider.value, Color(0,0,0,0.6))
func _on_mouse_exited() -> void:
	if button_pressed:
		timer.one_shot = true	
		timer.start(0.25)


func _on_mouse_entered() -> void:
	if is_instance_valid(timer):
		timer.stop()
"

