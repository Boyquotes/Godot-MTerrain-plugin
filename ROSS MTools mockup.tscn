@export var slider: VSlider
@export var textbox: LineEdit
var timer: Timer 
func _ready():
	timer = Timer.new()
	timer.timeout.connect(func(): button_pressed = false)
	add_child(timer)
	mouse_entered.connect(_on_mouse_entered)
	mouse_exited.connect(_on_mouse_exited)
func _toggled(toggled_on):
	if toggled_on:
		get_child(0).visible = true
		slider.value_changed.connect(update_value)
		textbox.text_submitted.connect(update_value)
	else:
		get_child(0).visible = false
		slider.value_changed.disconnect(update_value)
		textbox.text_submitted.disconnect(update_value)
func update_value(new_value):
	if slider.value != new_value: slider.value = new_value
	if textbox.text != str(new_value): textbox.text = str(new_value)	

#func _draw():
